"original":{"line":29,"column":29}},{"generated":{"line":79,"column":0},"source":"components/PubNoteCreate.js","original":{"line":29,"column":12}},{"generated":{"line":79,"column":12},"source":"components/PubNoteCreate.js","original":{"line":24,"column":8}},{"generated":{"line":79,"column":13},"source":"components/PubNoteCreate.js","original":{"line":23,"column":4}},{"generated":{"line":80,"column":0},"source":"components/PubNoteCreate.js","original":{"line":32,"column":1}},{"generated":{"line":80,"column":1},"source":"components/PubNoteCreate.js","original":{"line":6,"column":0}},{"name":"PrivNoteCreate","generated":{"line":82,"column":15},"source":"components/PubNoteCreate.js","original":{"line":34,"column":15}}],"sources":{"components/PubNoteCreate.js":"import React, { useState } from \"react\";\nimport MDEditor from '@uiw/react-md-editor';\nimport { Button } from \"react-bootstrap\";\n\n\nconst PrivNoteCreate = () => {\n    const [message, setMessage] = React.useState({ message: \"\" });\n    const [bmessage, bsetMessage] = React.useState();\n\n\n    const handleSubmit = e => {\n      e.preventDefault();\n      const requestOptions = {\n        method: \"POST\",\n        headers: { \"Content-Type\": \"application/json\" },\n        body: JSON.stringify({message: bmessage})\n      };\n      fetch(`${document.location.origin}/api/pub-message`, requestOptions)\n        .then(res => res.json())\n        .then(json => console.log(json));\n    };\n        \n    return(\n        <div>\n            <div className=\"header\"><a href=\"./notes\"><button className=\"backButton\">&#8249;</button></a></div>\n            <h1 className=\"SeTitle\">Create Public Note</h1>\n            <hr color=\"orange\" className=\"note-sep\" />\n            <MDEditor value={bmessage}  onChange={bsetMessage}/>\n            <Button onClick={handleSubmit}>Send</Button>\n        </div>\n    );\n};\n\nexport default PrivNoteCreate;\n"},"lineCount":null}},"error":null,"hash":"24423e2c3829f9d6de0b5